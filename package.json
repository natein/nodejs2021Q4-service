{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/natein/nodejs2021Q4-service"
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.ts",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.6.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "cross-env": "7.0.3",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.1.4",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.0.0",
    "uuid": "8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "supertest": "6.1.3",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2"
  }
}
